<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <script src="https://js.squareup.com/v2/paymentform.js" src_type="url"/>
        <script src="Squareup_Omni::js/squareup.js"/>
        <css src="Squareup_Omni::css/sqpaymentform.css"/>
    </head>
    <body>
        <referenceBlock name="order.create.billing.method.form">
            <action method="setMethodFormTemplate">
                <argument name="method" xsi:type="string">squareup_payment</argument>
                <argument name="template" xsi:type="string">Squareup_Omni::payment/form.phtml</argument>
            </action>
        </referenceBlock>
    </body>
</page>
