<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="admin-2columns-left"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="customer_form">
            <block class="Squareup\Omni\Block\Adminhtml\Edit\Tab\SquareTransactions" name="customer_edit_tab_transactions">
                <action method="setTabLabel">
                    <argument name="label" xsi:type="string" translate="true">Transactions</argument>
                </action>
            </block>
            <block class="Squareup\Omni\Block\Adminhtml\Edit\Tab\SquareRefunds" name="customer_edit_tab_refunds">
                <action method="setTabLabel">
                    <argument name="label" xsi:type="string" translate="true">Refunds</argument>
                </action>
            </block>
        </referenceBlock>
    </body>
</page>
