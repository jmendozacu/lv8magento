                    <item name="previewTmpl" xsi:type="string">Magento_Catalog/image-preview</item>
                    <item name="sortOrder" xsi:type="number">130</item>
                    <item name="uploaderConfig" xsi:type="array">
                        <item name="url" xsi:type="url" path="clogo/category_imageuploader_favicon/favicon"/>
                    </item>
					<item name="notice" xsi:type="string" translate="true"></item>
                     <item name="additionalClasses" xsi:type="string"></item>
					
                </item>
            </argument>
        </field>
				  
        <field name="navimage">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="dataType" xsi:type="string">string</item>
                    <item name="source" xsi:type="string">category</item>
                    <item name="label" xsi:type="string" translate="true">Navigation Image</item>
